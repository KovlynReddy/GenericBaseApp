@model GenericAppDLL.Models.ViewModels.ShopDashboardViewModel

<partial name="_SettingsImplementation" model="@Model.settings"></partial>


<div class="row" style="text-align: -webkit-right;">
    <h5>Points Balance :  <strong>@Model.TotalPoints</strong></h5>
</div>


@*<div class="col-2">
</div>
    <div class="col-8 overflow-auto">*@
@foreach (var vendor in Model.AllVendors)
{
    <div class="row overflow-auto p-3">
            <div class="col">
                @Html.Partial("_VendorView", vendor)
            </div>
        @foreach (var item in vendor.AllVendorItems)
        {
            <div class="col">
                @Html.Partial("_MenuItem", item)
            </div>
        }
    </div>
}
