@model GenericAppDLL.Models.ViewModels.ViewListMenuItems

<partial name="_SettingsImplementation" model="@Model.settings"></partial>


@{
    ViewData["Title"] = "ViewAll";
}

<table class="table">
    <thead>
        <tr>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.items)
        {
            <tr class="row">
                <td class="col-3">
                </td>
                <td class="col-6">
                    @Html.Partial("_MenuItem", item)
                </td>
                <td class="col-3">
                </td>
            </tr>
        }
    </tbody>
</table>

