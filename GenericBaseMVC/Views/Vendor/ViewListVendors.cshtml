@model IEnumerable<GenericAppDLL.Models.ViewModels.VendorViewModel>

@{
    ViewData["Title"] = "ViewListVendors";
}

<h1>ViewListVendors</h1>


    <table class="table">
        <thead>
            <tr>
                <th></th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="row">
                    <td class="col-3">
                    </td>
                    <td class="col-6">                          
                      @Html.Partial("_VendorView", item)   
                    </td>
                    <td class="col-3">
                    </td>
                </tr>

            }
        </tbody>
    </table>


